"use strict";$(document).ready(function(){$(document).on("click",".cl-tabs-control .cl-tab-control-item",function(n){if(n.preventDefault(),!$(this).hasClass("disabled")&&!$(this).hasClass("active")){$(this).closest(".cl-tabs-control").find(".active").removeClass("active");$(this).addClass("active");let n=$(this).text();$(this).closest(".wrap-cl-tabs-control").find(".mobile-dropdown-control").html(n+'<i class="ion-ios-arrow-down"><\/i>');$(".wrap-course-tabs").find(".cl-tab-item.active").removeClass("active");$(".wrap-course-tabs").find($(this).attr("href")).addClass("active");$(".mobile-dropdown-control-select option:selected").prop("selected",!1);$('.mobile-dropdown-control-select option[value="'+$(this).attr("href")+'"]').prop("selected",!1)}});if($(window).width()<557)$(".mobile-dropdown-control-select").on("change",function(){let n=$(this).val();$(".wrap-course-tabs").find(".cl-tab-item.active").removeClass("active");$(".wrap-course-tabs").find(n).addClass("active");$(".cl-tabs-control").find(".active").removeClass("active");$(".cl-tabs-control").find('a[href="'+n+'"]').addClass("active")})});translate.onLoad(function(){function i(){$("body").append(t)}var t="",n;t='<div class="wrap-start-tour-block"><div class="start-tour-content">        <h3 class="tour-heading">'+translate.instant("HOME_TOUR_1")+'<\/h3>        <h4 class="tour-des">'+translate.instant("HOME_TOUR_2")+'<\/h4>        <div class="tour-buttons"><a href="#" class="tour-btn continue-tour-btn" title="'+translate.instant("COURSE_TOURE_CONTINURE")+'">'+translate.instant("COURSE_TOURE_CONTINURE")+'<\/a><a href="#" class="tour-btn cancel-tour-btn" title="'+translate.instant("COURSE_TOURE_SKIP")+'">'+translate.instant("COURSE_TOURE_SKIP")+"<\/a><\/div>        <\/div><\/div>";getCookie("cl_home_tour")||$("body").append(t);$(document).on("click",".start-tour-content .tour-btn",function(t){t.preventDefault();$(this).hasClass("continue-tour-btn")?(n||(n=new Tour({debug:!0,backdrop:!0,backdropPadding:10,smartPlacement:!1,storage:!1,placement:"bottom",template:"<div class='popover tour cl-tour'>                    <h3 class='popover-title'><\/h3>                    <div class='popover-content'><\/div>                    <div class='popover-navigation'>                        <button class='tour-btn sec-tour-btn' data-role='prev'>"+translate.instant("COURSE_TOURE_BACK")+"<\/button>                        <button class='tour-btn' data-role='next'>"+translate.instant("COURSE_TOURE_CONTINURE")+"<\/button>                        <button class='tour-btn finish-tour disabled' data-role='end'>"+translate.instant("COURSE_TOURE_COMPLETE")+"<\/button>                        <button class='tour-btn cancel-tour-btn' data-role='end'>"+translate.instant("COURSE_TOURE_SKIP")+"<\/button>                    <\/div>                    <\/div> ",onEnd:function(){$("body").removeClass("on-tour");getCookie("cl_home_tour")||setCookie("cl_home_tour",1,30)}}),$(".site-user-menu #login-header")[0]&&n.addStep({element:".site-user-menu",title:translate.instant("HOME_TOUR_LOGIN"),placement:$(window).width()>992?"left":"bottom"}),$(".wrap-main-user-profile")[0]&&(n.addStep({element:".main-user-info",title:translate.instant("HOME_TOUR_LEVEL"),placement:$(window).width()>992?"right":"bottom"}),n.addStep({element:".wrap-detail-user-progress>.row",title:translate.instant("HOME_TOUR_DASHBOARD"),placement:"bottom"}),n.addStep({element:".reward-block",title:translate.instant("HOME_TOUR_REWARD"),placement:"bottom"})),$(window).width()>556?(n.addStep({element:".wrap-block-courses .cl-tabs-control li:first-child",title:translate.instant("HOME_TOUR_COURSE_SUGGEST"),placement:$(window).width()>992?"right":"bottom"}),n.addStep({element:".wrap-block-courses .cl-tabs-control li:nth-child(2)",title:translate.instant("HOME_TOUR_COURSE_RECENT"),placement:$(window).width()>992?"right":"bottom"}),n.addStep({element:".wrap-block-courses .cl-tabs-control li:nth-child(3)",title:translate.instant("HOME_TOUR_COURSE_FINISH"),placement:$(window).width()>992?"right":"bottom"})):n.addStep({element:".wrap-block-courses .container",title:translate.instant("HOME_TOUR_COURSES"),placement:"top"}),n.addStep({element:".code-fights-section>.container",title:translate.instant("HOME_TOUR_CODE_FIGHT"),placement:"top"}),n.addStep({element:".mid-bottom-section .col-xs-12.col-lg-7",title:translate.instant("HOME_TOUR_TRAINING"),placement:$(window).width()>1200?"right":"top"}),$(".mid-bottom-section .col-xs-12.col-lg-5 .blog-item")[0]&&n.addStep({element:".mid-bottom-section .col-xs-12.col-lg-5",title:translate.instant("HOME_TOUR_BLOG"),placement:$(window).width()>1200?"left":"top"}),n.addStep({element:".bottom-section .col-xs-12.col-md-8",title:translate.instant("HOME_TOUR_SKILLS"),placement:$(window).width()>992?"right":"bottom"}),n.addStep({element:".bottom-section .col-xs-12.col-md-4",title:translate.instant("HOME_TOUR_ACTS"),placement:$(window).width()>992?"left":"bottom"})),n.ended()&&n.init(),setTimeout(function(){n.ended()?n.restart():n.start()},300),$("body").addClass("on-tour")):setCookie("cl_home_tour",1,30);$(".wrap-start-tour-block").fadeOut()});$(document).on("click",".open-tour",function(n){n.preventDefault();$(".wrap-start-tour-block")[0]?$(".wrap-start-tour-block").fadeIn():i()})});$(document).ready(function(){var n=0;$(window).resize(function(){n!=$(window).width()&&(n=$(window).width(),setTimeout(function(){var n=1;n=parseInt($(".list-rewards").width()/60);$(".slick-slider.list-rewards").slick("unslick");$(".list-rewards").slick({dots:!1,arrows:!0,infinite:!0,swipeToSlide:!0,speed:300,slidesToShow:n,slidesToScroll:1,nextArrow:'<span class="carousel-btn next-slide cl-icon-angle-right"><\/span>',prevArrow:'<span class="carousel-btn prev-slide cl-icon-angle-left"><\/span>'})},100))}).resize()});